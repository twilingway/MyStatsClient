{
  "compilerOptions": {
    "plugins": [
      {
        "name": "typescript-plugin-css-modules"
      }
    ],
    "baseUrl": "src",
    "module": "commonjs",
    "target": "es5",
    "jsx": "react-jsx",
    "strict": true,
    "outDir": "./dist",
    "allowJs": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "dist", "tsconfig.json", "eslintrc.js"]
}
